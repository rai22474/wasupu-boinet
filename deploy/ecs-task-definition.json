{
    "containerDefinitions": [
        {
            "name": "boinet",
            "image": "{{AWS_DOCKER_REGISTRY}}/boinet:latest",
            "cpu": 100,
            "memory": 500,
            "essential": true,
            "portMappings": [],
            "environment": [
                {
                    "name": "SEMAAS_API_KEY",
                    "value": "{{SEMAAS_API_KEY}}"
                },
                {
                    "name": "SEMAAS_NAMESPACE",
                    "value": "{{SEMAAS_NAMESPACE}}"
                }
            ],
            "links": [],
            "extraHosts": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "{{AWSLOGS_GROUP}}",
                    "awslogs-region": "{{AWSLOGS_REGION}}",
                    "awslogs-stream-prefix": "boinet"
                }
            }
        }
    ]
}
